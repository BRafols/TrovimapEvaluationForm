{
  "name": "vue-trovimap-wizard",
  "version": "1.0.13",
  "description": "A Vue component for inputting Naira-formatted amounts.",
  "main": "dist/vue-trovimap-wizard.ssr.js",
  "module": "dist/vue-trovimap-wizard.esm.js",
  "unpkg": "dist/vue-trovimap-wizard.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "author": "Uk Chukundah",
  "license": "MIT",
  "keywords": [
    "vue",
    "currency",
    "input",
    "naira"
  ],
  "dependencies": {
    "axios": "^0.19.0",
    "text-mask-addons": "^3.8.0",
    "vue-text-mask": "^6.1.2",
    "vuetify": "^1.5.20",
    "vuex": "^3.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ukchukx/vue-trovimap-wizard"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "rollup": "^1.12.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^5.0.0",
    "sass-loader": "^8.0.0",
    "stylus-loader": "^3.0.2",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
